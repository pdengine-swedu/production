
    <div class="container">

      <div class="starter-template">
        <h1>Bootstrap starter template</h1>
        <p class="lead">Use this document as a way to quickly start any new project.<br> All you get is this text and a mostly barebones HTML document.</p>
      </div>
      <div class="row">
        <div class="col-md-4 facetNav">
          <span><h2>Facets</h2></span>
          <a href="/seminars?blah[]=Technology Coaching&blah[]=ipad">Facets</a>

          
        </div>
        <div class="col-md-7 listing">
          <div class="panel-group" id="accordion">
            <% @seminars.each do |seminar| %>
              <div class="panel panel-default">
                <div class="panel-heading">
                  <h4 class="panel-title">
                    <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href=<%= '#' + seminar.id %> >
                      <h2><%= seminar.title %></h2>
                    </a>
                  </h4>
                </div>
                <div class="panel-collapse collapse" id=<%= seminar.id %> >
                  <div class="panel-body">
                    <%= seminar.description %>
                    <% seminar.tags.each do |tag| %>
                      <%= tag.title %>
                    <% end %>
                  </div>
                </div>
              </div>
            <% end %>
          </div>
        </div>
      </div>

    </div><!-- /.container -->


